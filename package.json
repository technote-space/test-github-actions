{
    "name": "test-github-actions",
    "version": "0.0.22",
    "description": "Test GitHub Actions",
    "license": "MIT",
    "author": "Technote <technote.space@gmail.com> (https://technote.space)",
    "scripts": {
        "build": "webpack --mode production --config webpack.config.js",
        "cover": "jest --coverage",
        "cover:update": "jest --coverage --updateSnapshot",
        "develop": "webpack --mode development --config webpack.config.js",
        "lint": "eslint 'resources/js/**/*.js'",
        "start": "yarn build",
        "stylelint": "stylelint 'resources/sass/**/*.scss'",
        "test": "yarn lint && yarn cover",
        "test:update": "yarn lint && yarn cover:update",
        "update": "ncu -u --packageFile package.json && yarn install && yarn upgrade && yarn audit"
    },
    "dependencies": {
        "classnames": "^2.2.6"
    },
    "devDependencies": {
        "@babel/core": "^7.12.3",
        "@babel/plugin-proposal-object-rest-spread": "^7.12.1",
        "@babel/plugin-transform-runtime": "^7.12.1",
        "@babel/preset-env": "^7.12.1",
        "@babel/register": "^7.12.1",
        "babel-jest": "^26.5.2",
        "babel-loader": "^8.1.0",
        "css-loader": "^5.0.0",
        "duplicate-package-checker-webpack-plugin": "^3.0.0",
        "eslint": "^7.11.0",
        "jest": "^26.5.3",
        "speed-measure-webpack-plugin": "^1.3.3",
        "stylelint": "^13.7.2",
        "stylelint-config-standard": "^20.0.0",
        "stylelint-order": "^4.1.0",
        "stylelint-scss": "^3.18.0",
        "webpack": "^5.1.3",
        "webpack-cli": "^4.0.0"
    }
}
